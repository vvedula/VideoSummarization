/**
 * @author Giulio
 */
public class AudioException extends Exception {

    public AudioException(String message) {
	super(message);
    }

    public AudioException(Throwable cause) {
	super(cause);
    }

    public AudioException(String message, Throwable cause) {
	super(message, cause);
    }

}
